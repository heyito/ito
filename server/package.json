{
  "name": "ito-server",
  "version": "1.0.0",
  "main": "dist/index.js",
  "type": "module",
  "license": "GPL-3.0-only",
  "dependencies": {
    "@auth0/auth0-fastify-api": "^1.0.2",
    "@bufbuild/buf": "^1.55.1",
    "@bufbuild/protobuf": "^2.5.2",
    "@bufbuild/protoc-gen-es": "^2.5.2",
    "@connectrpc/connect": "^2.0.2",
    "@connectrpc/connect-fastify": "^2.0.2",
    "@connectrpc/connect-node": "^2.0.2",
    "@connectrpc/protoc-gen-connect-es": "^1.6.1",
    "@grpc/grpc-js": "^1.13.4",
    "@grpc/proto-loader": "^0.7.15",
    "@grpc/reflection": "^1.0.4",
    "@types/jsonwebtoken": "^9.0.10",
    "dotenv": "^16.5.0",
    "express": "^5.1.0",
    "express-oauth2-jwt-bearer": "^1.6.1",
    "express-rate-limit": "^7.5.0",
    "fastify": "^5.4.0",
    "grpc-health-check": "^2.0.2",
    "jsonwebtoken": "^9.0.2",
    "jwks-rsa": "^3.2.0",
    "pg": "^8.16.0"
  },
  "scripts": {
    "build": "tsc",
    "clean": "rm -rf dist",
    "start": "node dist/index.js",
    "proto:gen:server": "proto-loader-gen-types --importFileExtension .js --grpcLib=@grpc/grpc-js --outDir=src/generated/ src/ito.proto node_modules/grpc-health-check/proto/health/v1/health.proto",
    "proto:gen:client": "proto-loader-gen-types --importFileExtension .js --grpcLib=@grpc/grpc-js --outDir=../app/src/generated/ src/ito.proto node_modules/grpc-health-check/proto/health/v1/health.proto",
    "proto:gen": "bun run proto:gen:server && bun run proto:gen:client",
    "dev": "tsx watch src/index.ts",
    "test-client": "tsx ./test-client.ts",
    "test-auth": "tsx ./test-auth-client.ts",
    "test-connect": "tsx ./test-connect-client.ts"
  },
  "devDependencies": {
    "@types/express": "^5.0.3",
    "@types/node": "^24.0.1",
    "@types/pg": "^8.15.4",
    "prettier": "^3.5.3",
    "ts-node": "^10.9.2",
    "tsx": "^4.20.2",
    "typescript": "^5.8.3"
  }
}
